{
  "compilerOptions": {
    "module": "ES6",
    "target": "ESNext",
    "baseUrl": "/app/",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "NodeNext",
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "importsNotUsedAsValues": "error"
  },
  "exclude": [
    "node_modules"
  ]
}
